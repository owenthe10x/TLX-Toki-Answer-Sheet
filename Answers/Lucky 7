/*
  LUCKY 7
Time limit : 1s
Memory limit : 64mb

Description
It can be proved that for all N ≥ 7, there exists at least an array A which satisfies the following conditions:
All elements of A are positive integers.
The sum of all elements of A is N.
A is sorted in non-decreasing order.
If K is the length of A, then AK - A1 = 1.
Given N, output any array A that satisfies the conditions.

Input Format
The input is given in the following format:
N
Output Format
The first line is a single integer K denoting the length of A.

The second line is K integers denoting A1, A2, ..., AK.

Sample Input
7
Sample Output
2
3 4

Explanation of Sample
One of other possible output that satisfies the rules is:
3
2 2 3
Constraints
7 ≤ N ≤ 10,000
*/

#include <iostream>
#include <cmath>
#include <string>
#include <array>
#include <fstream>
#include <iomanip>
#include <cstring>

using namespace std;
int n, length, temp;

int main()
{
    cin >> n;
    if (n >= 7 && n <= 10000)
    {
        if (n % 2 == 0)
        {
            if (n % 3 != 0)
            {
                int arr[3];
                temp = n % 3;
                for (int j = 2; j > 2 - temp; j--)
                {
                    arr[j] = floor(n / 3) + 1;
                }
                for (int k = 0; k < 3 - temp; k++)
                {
                    arr[k] = floor(n / 3);
                }
                cout << 3 << endl;
                for (int i = 0; i < 3; i++)
                {
                    cout << arr[i] << " ";
                }
            }
            else if (n % 5 != 0)
            {
                int arr[5];
                temp = n % 5;
                for (int j = 4; j > 4 - temp; j--)
                {
                    arr[j] = floor(n / 5) + 1;
                }
                for (int k = 0; k < 5 - temp; k++)
                {
                    arr[k] = floor(n / 5);
                }
                cout << 5 << endl;
                for (int i = 0; i < 5; i++)
                {
                    cout << arr[i] << " ";
                }
            }
            else if (n % 7 != 0)
            {
                int arr[7];
                temp = n % 7;
                for (int j = 6; j > 6 - temp; j--)
                {
                    arr[j] = floor(n / 7) + 1;
                }
                for (int k = 0; k < 7 - temp; k++)
                {
                    arr[k] = floor(n / 7);
                }
                cout << 7 << endl;
                for (int i = 0; i < 7; i++)
                {
                    cout << arr[i] << " ";
                }
            }
            else if (n % 11 != 0)
            {
                int arr[11];
                temp = n % 11;
                for (int j = 10; j > 10 - temp; j--)
                {
                    arr[j] = floor(n / 11) + 1;
                }
                for (int k = 0; k < 11 - temp; k++)
                {
                    arr[k] = floor(n / 11);
                }
                cout << 11 << endl;
                for (int i = 0; i < 11; i++)
                {
                    cout << arr[i] << " ";
                }
            }
        }
        else
        {
            int arr[2];
            arr[0] = floor(n / 2);
            arr[1] = floor(n / 2) + 1;
            cout << 2 << endl;
            for (int i = 0; i < 2; i++)
            {
                cout << arr[i] << " ";
            }
        }
    }
}
